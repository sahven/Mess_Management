<div class="form-group">
	    
	    <label for="first">First Name</label>
	    
	    <input type="text" class="form-control" id="first" placeholder="" name="firstname">
	    
	  </div>
	  
	  <div class="form-group">
	    
	    <label for="last">Last Name</label>
	    
	    <input type="text" class="form-control" id="last" placeholder="" name="lastname">
	    
	  </div>

	  <div class="form-group">
	    
	    <label for="roll">Roll Number</label>
	    
	    <input type="number" class="form-control" id="roll" placeholder="" name="rollno">
	    
	  </div>

	  <div class="form-group">
	    
